pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
              echo 'Building..'
            }
        }
        stage('Load') {
            code = load 'pipeline.groovy'
             }
        stage('Test') {
            steps {
                echo 'Testing..'
                exampleModule.greetMsg() 
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}
