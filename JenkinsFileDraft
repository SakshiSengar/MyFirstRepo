pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
              echo 'Building..'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
              //  exampleModule.greetMsg() 
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}
node {
   // def rootDir = PATH
    def exampleModule = load "./pipeline.groovy"
    exampleModule.greetMsg()
   println  assert exampleModule.divide(12, 3) instanceof BigDecimal
}
